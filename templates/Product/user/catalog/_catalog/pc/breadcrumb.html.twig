

		<nav class="small" style="--bs-breadcrumb-divider: '-';" aria-label="breadcrumb">
			<ol class="breadcrumb text-secondary small opacity-75">
				<li>
					<svg xmlns="http://www.w3.org/2000/svg" width="10" height="10" fill="currentColor" class="bi bi-caret-right-fill text-muted me-1" viewBox="0 0 16 16">
						<path d="m12.14 8.753-5.482 4.796c-.646.566-1.658.106-1.658-.753V3.204a1 1 0 0 1 1.659-.753l5.48 4.796a1 1 0 0 1 0 1.506z"/>
					</svg>
				</li>

				<li class="breadcrumb-item">
					<a href="{{ path('Pages:user.homepage') }}"
					   class="text-muted"
					   aria-label="{{ 'breadcrumb.homepage.user'|trans({}, 'pages') }}"
					   title="{{ 'breadcrumb.homepage.user'|trans({}, 'pages') }}">
						{{ 'breadcrumb.homepage.user'|trans({}, 'pages') }}
					</a>
				</li>
				
				{% if category.parent_category_name %}
					<li class="breadcrumb-item">
						
						<a href="{{ path('Product:user.catalog.category', { url : category.parent_category_url }) }}"
								class="text-muted"
								aria-label="{{ category.parent_category_name }}"
								title="{{ category.parent_category_name }}">
							{{ category.parent_category_name }}
						</a>
					</li>
				{% endif %}
				
				

				<li class="breadcrumb-item active text-primary"
					aria-current="page" aria-label="{{ category.category_name }}"
					title="{{ category.category_name }}">{{ category.category_name }}</li>
			</ol>
		</nav>
